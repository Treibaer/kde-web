<div class="container">
    <table class="table table-dark">
        <thead>
        <tr>
            <th scope="col">Date</th>
            <th scope="col">Author</th>
            <th scope="col">Content</th>
        </tr>
        </thead>
        <tbody>
        {% for commit in commits %}
            <tr>
                <td>{{ commit.date }}</td>
                <td>{{ commit.author }}</td>
                <td>{{ commit.content }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
